{
  "name": "ChronoLog",
  "version": "1.0.1",
  "description": "Cross-platform real-time logger for embedded systems (ESP32, STM32, nRF52)",
  "keywords": [
    "logger",
    "rtos",
    "uart",
    "debug",
    "platformio",
    "esp32",
    "stm32",
    "nrf52",
    "arduino"
  ],
  "authors": [
    {
      "name": "Hamas Saeed",
      "email": "hamasaeed888@gmail.com",
      "url": "https://github.com/Hamas888",
      "maintainer": true
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Hamas888/ChronoLog.git"
  },
  "frameworks": ["arduino", "espidf"],
  "platforms": ["atmelavr","espressif32", "ststm32", "nordicnrf52"],
  "build": {
    "flags": [
      "-std=gnu++17"
    ],
    "unflags": [
      "-std=gnu++11"
    ]
  },
  "license": "MIT",
  "homepage": "https://github.com/Hamas888/ChronoLog",
  "examples": [
    "examples/ESP-IDF/ESP32-C6 Logging/main/main.cpp",
    "examples/ESP-IDF/ESP32-S3 Logging/main/main.cpp",
    "examples/STM32Cube/STM32_NucleoL010RB_Logging/Core/Src/main.cpp",
    "examples/STM32Cube/STM32_NucleoL010RB_Logging_RTOS/Core/Src/main.cpp",
    "examples/PlatformIO/Arduino/ESP32-C6 Logging/src/main.cpp",
    "examples/PlatformIO/Arduino/ESP32-S3 Logging/src/main.cpp",
    "examples/PlatformIO/ESP-IDF/ESP32-C6 Logging/src/main.cpp",
    "examples/PlatformIO/ESP-IDF/ESP32-S3 Logging/src/main.cpp",
    "examples/ArduinoIDE/ArduinoUno-Logging/ArduinoUno-Logging.ino",
    "examples/NRF Connect SDK/nRF52832_Logging/src/main.cpp"
  ],
  "export": {
    "include": [
      "src",
      "include",
      "examples",
      "CMakeLists.txt",
      "library.json",
      "LICENSE",
      "README.md"
    ],
    "exclude": [
      "test",
      ".pio",
      ".vscode"
    ]
  }
}